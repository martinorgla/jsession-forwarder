{
  "manifest_version": 3,
  "name": "JSession Forwarder",
  "version": "1.1",
  "description": "Reads the JSESSIONID and XSRF-TOKEN cookies and sends their values to the specified endpoint.",
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Session Forwarder",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png"
  }
}
